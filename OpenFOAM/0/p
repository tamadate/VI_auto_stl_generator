{
    inlet
    {
        type            fixedValue;
        value           uniform 1e5;
    }
    majorOutlet
    {
        type            fixedValue;
        value           uniform 0.6e5;    
	}
    minorOutlet
    {
        type            zeroGradient;

    }
    conversion
    {
        type            zeroGradient;
    }
    conv_to_nozzle
    {
        type            zeroGradient;
    }
    nozzle
    {
        type            zeroGradient;
    }

	nozzle_bottom
    {
        type            zeroGradient;
    }
	nozzle_out
    {
        type            zeroGradient;
    }
	major_top
    {
        type            zeroGradient;
    }
	major_side
    {
        type            zeroGradient;
    }
	majorOut_out
    {
        type            zeroGradient;
    }
	majorOut_in
    {
        type            zeroGradient;
    }		
	majorOut_curve
    {
        type            zeroGradient;
    }								
	major_bottom
    {
        type            zeroGradient;
    }
	recieving_out
    {
        type            zeroGradient;
    }
	recieving_top
    {
        type            zeroGradient;
    }
	recieving
    {
        type            zeroGradient;
    }
	recieving_expand
    {
        type            zeroGradient;
    }
	minor_wall
    {
        type            zeroGradient;
    }
	axis
    {
		type symmetryPlane;
    }
	topEmptyFaces
	{
		type empty;
	}
	bottomEmptyFaces
	{
		type empty;
	}

 
}


